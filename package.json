{
  "private": true,
  "name": "remix-app-template-js",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "postinstall": "remix setup vercel",
    "prettify": "prettier --write app"
  },
  "dependencies": {
    "@remix-run/react": "^2.16.0",
    "@remix-run/serve": "^2.16.0",
    "@remix-run/vercel": "^1.19.3",
    "date-fns": "^2.28.0",
    "gray-matter": "^4.0.3",
    "node-fetch": "^3.2.0",
    "prettier": "^2.5.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^6.0.3",
    "react-syntax-highlighter": "^15.4.5",
    "rehype-raw": "^5.1.0",
    "remix": "^2.16.0",
    "slug": "^5.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.16.0",
    "@remix-run/v1-route-convention": "^0.1.4",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.18"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
